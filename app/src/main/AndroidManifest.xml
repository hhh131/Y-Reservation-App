<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".webView"></activity>
        <activity
            android:name=".Room.Willow"
            android:parentActivityName=".List" />
        <activity
            android:name=".Room.PcZoneActivity"
            android:parentActivityName=".List" />
        <activity
            android:name=".ReportActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Room.seminar"
            android:parentActivityName=".List" />
        <activity
            android:name=".List"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".JoinActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".Room.QuietZone"
            android:parentActivityName=".List" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Room.DVDZone"
            android:parentActivityName=".List" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>